{
    "name": "geo-sot/laravel-adminbasefiles",
    "description": "A laravel Package that creates a backend",
    "keywords": [
        "geo-sot",
        "laravel",
        "laravel-adminBase"
    ],
    "authors": [
        {
            "name": "Geo Sot",
            "email": "geo.sotis@gmail.com"
        }
    ],
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^7.3",
        "laravel/fortify": "^1.6",
        "barryvdh/laravel-debugbar": "^v3.5.1",
        "barryvdh/laravel-translation-manager": "^v0.5.9",
        "cviebrock/eloquent-sluggable": "^8.0.0",
        "geo-sot/laravel-env-editor": "^0.9.4",
        "kris/laravel-form-builder": "^1.40.0",
        "lab404/laravel-impersonate": "^1.7.1",
        "laravel/passport": "^10.0.1",
        "mcamara/laravel-localization": "^1.6.0",

        "plank/laravel-mediable": "^5",
        "rap2hpoutre/laravel-log-viewer": "^1.4",
        "santigarcor/laratrust": "^6.2.2",
        "spatie/laravel-translatable": "^4.4.1",
        "venturecraft/revisionable": "1.*"
    },
    "autoload": {
        "psr-4": {
            "GeoSot\\BaseAdmin\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
        },
        "laravel": {
            "providers": [
                "GeoSot\\BaseAdmin\\ServiceProvider"
            ]
        }
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^v2.8.1",
        "laravel/framework": "6.x|7.*|8.*",
        "phpunit/phpunit": "^9.3"
    },
    "autoload-dev": {
        "psr-4": {
            "GeoSot\\BaseAdmin\\Tests\\": "tests/"
        }
    }
}
